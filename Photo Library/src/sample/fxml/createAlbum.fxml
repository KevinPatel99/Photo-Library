<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="rootPane" prefHeight="236.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.CreateAlbumController">
   <children>
      <Label alignment="CENTER" layoutX="124.0" layoutY="50.0" text="Create Album" />
      <Button fx:id="createButton" layoutX="124.0" layoutY="118.0" mnemonicParsing="false" onAction="#createButtonEvent" prefHeight="32.0" prefWidth="62.0" text="Create" />
      <TextField fx:id="newAlbumNameField" alignment="CENTER" layoutX="81.0" layoutY="78.0" onAction="#newAlbumNameEvent" promptText="Enter New Album Name" />
      <Button fx:id="quitButton" layoutX="124.0" layoutY="161.0" mnemonicParsing="false" onAction="#quitButtonEvent" prefHeight="32.0" prefWidth="62.0" text="Quit" />
   </children>
</AnchorPane>
